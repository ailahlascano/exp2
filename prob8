#include <iostream>

using namespace std;
int main()
{
    int num;
    
    cout << "Enter a number: ";
    cin >> num;
    
    cout << "Multiplication table of: " << num;
    cout << "\n\n";
    
    cout << "1 * " << num << " = " << 1 * num;
    cout << "\n";
    cout << "2 * " << num << " = " << 2 * num;
    cout << "\n";
    cout << "3 * " << num << " = " << 3 * num;
    cout << "\n";
    cout << "4 * " << num << " = " << 4 * num;
    cout << "\n";
    cout << "5 * " << num << " = " << 5 * num;
    cout << "\n";
    cout << "6 * " << num << " = " << 6 * num;
    cout << "\n";
    cout << "7 * " << num << " = " << 7 * num;
    cout << "\n";
    cout << "8 * " << num << " = " << 8 * num;
    cout << "\n";
    cout << "9 * " << num << " = " << 9 * num;
    cout << "\n";
    cout << "10 * " << num << " = " << 10 * num;
    cout << "\n";
    
    return 0;
    
}
